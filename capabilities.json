{
  "dataRoles": [
    {
      "displayName": "Target Variable",
      "kind": "GroupingOrMeasure",
      "name": "Target"
    },
	{
      "displayName": "Input Variables",
      "kind": "Grouping",
      "name": "Variables"
    }
  ],
  "dataViewMappings": [
    {
	 "conditions": [
           { "Target": { "max": 1 }, "Variables": { "max": 1000 } }
      ],

      "scriptResult": {
        "dataInput": {
          "table": {
            "rows": {
                "select": [
                { "for": { "in": "Target" } },
                { "for": { "in": "Variables" } }
              ],
              "dataReductionAlgorithm": {
                "top": { }
              }
            }
          }
        },
        "script": {
          "scriptProviderDefault": "R",
          "scriptOutputType": "png",
          "source": {
            "objectName": "rcv_script",
            "propertyName": "source"
          },
          "provider": {
            "objectName": "rcv_script",
            "propertyName": "provider"
          }
        }
      }
    }
  ],
  "objects": {
    "rcv_script": {
      "properties": {
        "provider": {
          "type": { "text": true }
        },
        "source": {
          "type": {
            "scripting": { "source": true }
          }
        }
      }
    },
